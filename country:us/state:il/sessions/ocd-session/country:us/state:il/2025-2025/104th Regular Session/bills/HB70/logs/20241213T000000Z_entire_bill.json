  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Property Tax Code. Increases the amount of the homestead exemption for persons with disabilities from $2,000 to $4,000 beginning in tax year 2025. Effective immediately."
    }
  ],
