{
  "legislative_session": "104th",
  "identifier": "HR124",
  "title": "TARDIVE DYSKINESIA AWARE WEEK",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Declares May 4 through May 10, 2025 as Tardive Dyskinesia Awareness Week (TDAW) in the State of Illinois. Expresses support for TDAW in recognition of the importance of early detection and intervention to improve outcomes for people living with mental health conditions and prescribed antipsychotics. Expresses support towards efforts to raise awareness about the causes and symptoms of tardive dyskinesia (TD) and the importance of routine TD screening."
    }
  ],
