  "title": "OPEN MTGS-NOTICE OF CHANGES",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Open Meetings Act. In provisions regarding notice of changes to regular meeting dates, deletes requirements for publication in a newspaper or, in certain cases, posting in at least 3 prominent places within the governmental unit. Adds a requirement that notice of changes to regular meeting dates shall also be posted on the website of the public body."
    }
  ],
