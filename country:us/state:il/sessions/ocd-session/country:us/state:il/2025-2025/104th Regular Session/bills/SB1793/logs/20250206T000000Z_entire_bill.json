{
  "legislative_session": "104th",
  "identifier": "SB1793",
  "title": "CREMATION-SCATTERING AREA",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Crematory Regulation Act. Requires the Department of Natural Resources to designate scattering areas for the deposition of cremated remains in Illinois rivers in accordance with a Department-granted permit."
    }
  ],
