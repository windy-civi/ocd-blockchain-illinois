  "title": "SCHOOL CASELOAD REVIEW",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Directs the Professional Review Panel and the Illinois State Board of Education to conduct the analysis and financial modeling required to evaluate the implications of implementing the recommended maximum caseloads for special education resource teachers. Requests the Professional Review Panel and the Illinois State Board of Education to submit their findings in a written report to the General Assembly and Governor by December 31, 2026."
    }
  ],
