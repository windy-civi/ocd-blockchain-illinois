  "title": "SWIMMING FACILITY-CONSTRUCTION",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Swimming Facility Act. Provides that permits for construction or major alteration of a swimming facility are valid for a period of 2 years (rather than one year) from the date of issue."
    }
