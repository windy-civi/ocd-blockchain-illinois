  "identifier": "SB1339",
  "title": "GIANT CITY STATE PARK",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the State Parks Designation Act. Provides that the visitor center located in Giant City State Park shall be known as the Bob Kristoff Visitor Center. Specifies that the arena located at Giant City Stables in Giant City State Park shall be known as the Richard A. Kelly Arena. Effective immediately."
    }
  ],
