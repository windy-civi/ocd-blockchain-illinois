  "title": "LITTLE PALESTINE DAY",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Declares April 7, 2025 as Little Palestine Day in the State of Illinois. Urges Illinoisans to celebrate Little Palestine Day by supporting businesses and cultural initiatives in Little Palestine and attending or hosting events showcasing the contributions of the Arab American community to the United States."
    }
  ],
