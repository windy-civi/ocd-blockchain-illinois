  "identifier": "HR304",
  "title": "ALS AWARENESS MONTH",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Declares May 2025 as ALS Awareness Month in the State of Illinois. Calls upon all Illinoisans to join in supporting amyotrophic lateral sclerosis (ALS) research and advocating for increased funding and to stand in solidarity with those affected by this relentless disease. Expresses earnest support for the ALS community as they seek to increase awareness, prioritize research funding, and secure additional caregiving support."
    }
  ],
