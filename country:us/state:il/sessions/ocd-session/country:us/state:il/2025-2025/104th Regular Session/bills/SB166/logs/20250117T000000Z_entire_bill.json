{
  "legislative_session": "104th",
  "identifier": "SB166",
  "title": "DCFS-FOSTER PAYMENT SCHEDULE",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Foster Parent Law. Provides that foster parents have a right to receive an initial payment of $480 per child to be made within 21 days of a permanent placement. Provides that if the final monthly payment for a child amounts to $480 or more, the amount of $480 shall be deducted from the final monthly payment. Provides that if the final monthly payment amounts to less than $480, the final monthly payment shall not be made."
    }
  ],
