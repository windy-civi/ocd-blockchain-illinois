{
  "legislative_session": "104th",
  "identifier": "HB18",
  "title": "FOID-DENIAL & REVOC-STALKING",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Firearm Owners Identification Card Act. Provides that the Illinois State Police has authority to deny an application for or to revoke and seize a Firearm Owner's Identification Card previously issued under the Act if the Illinois State Police finds that the applicant or the person to whom the card was issued is or was at the time of issuance a person who has been convicted of misdemeanor stalking in another jurisdiction or a similar misdemeanor offense in another jurisdiction."
    }
  ],
