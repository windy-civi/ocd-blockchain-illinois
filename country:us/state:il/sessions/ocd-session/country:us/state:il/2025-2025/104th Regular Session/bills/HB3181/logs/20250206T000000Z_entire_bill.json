  "title": "INC TX-EMERGENCY WORKER",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Income Tax Act. In provisions concerning volunteer emergency workers, provides that, for taxable years beginning on or after January 1, 2026, \"volunteer emergency worker\" also includes a community policing volunteer, a volunteer auxiliary police officer, or a volunteer auxiliary deputy. Effective immediately."
    }
  ],
