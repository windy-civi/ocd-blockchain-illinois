{
  "legislative_session": "104th",
  "identifier": "HB2994",
  "title": "MENTAL HEALTH RECORDS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Mental Health and Developmental Disabilities Confidentiality Act. Provides those entitled to inspect and copy a recipient's record include the parent or guardian of a minor, regardless of the minor's age, who is involved in special education services under the School Code or the \"designated representative\" of a student over the age of 18 involved in special education services under the School Code."
    }
  ],
