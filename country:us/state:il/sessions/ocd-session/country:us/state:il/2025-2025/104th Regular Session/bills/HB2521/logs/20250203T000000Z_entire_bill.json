  "legislative_session": "104th",
  "identifier": "HB2521",
  "title": "SCH CD-CRIMINAL HISTORY CHECK",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the School Code. Includes game officials in provisions requiring applicants for employment with a school district to undergo a fingerprint-based criminal history records check and check of the Statewide Sex Offender Database and Statewide Murderer and Violent Offender Against Youth Database."
    }
  ],
