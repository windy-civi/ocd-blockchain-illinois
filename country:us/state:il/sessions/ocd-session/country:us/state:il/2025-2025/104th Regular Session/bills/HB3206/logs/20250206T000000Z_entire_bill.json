  "title": "VEH-FLEE/ELUDE OFFICER-PENALTY",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Increases the penalty for fleeing or attempting to elude a peace officer from a Class A misdemeanor to a Class 4 felony, and, for a third or subsequent violation, from a Class 4 felony to a Class 3 felony."
    }
  ],
