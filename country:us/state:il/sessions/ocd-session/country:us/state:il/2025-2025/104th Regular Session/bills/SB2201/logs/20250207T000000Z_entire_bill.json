{
  "legislative_session": "104th",
  "identifier": "SB2201",
  "title": "CD CORR-DOC-REPORT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Unified Code of Corrections. Provides that the Department of Corrections annually shall collect and publish on its website specified data concerning Department of Corrections facilities. Provides that this data includes: (1) contraband found by facility; (2) substance use disorder treatment or educational programming data by facility; (3) emergency medical response and hospitalizations by facility; and (4) overdoses by facility."
    }
  ],
