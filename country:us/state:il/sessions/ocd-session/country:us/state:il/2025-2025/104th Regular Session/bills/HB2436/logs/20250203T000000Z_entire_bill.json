{
  "legislative_session": "104th",
  "identifier": "HB2436",
  "title": "PUBLIC DEFENDER-COOK COUNTY",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Counties Code. Provides that, in counties with a population over 3,000,000, representation by the public defender in immigration cases shall be limited to those arising or being heard within the geographical boundaries of the county where the public defender has been appointed to office and to those of county residents with immigration cases outside of the county unless the county board authorizes the public defender to provide representation beyond those limits (rather than limited to those arising in immigration courts located within the geographical boundaries of the county where the public defender has been appointed to office unless the county board authorizes the public defender to provide representation outside the county)."
    }
  ],
