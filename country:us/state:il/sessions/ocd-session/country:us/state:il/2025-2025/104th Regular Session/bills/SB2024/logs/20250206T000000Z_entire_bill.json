  "identifier": "SB2024",
  "title": "INC TX-DIESEL CREDIT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Income Tax Act. Creates an income tax credit for each taxpayer that manufactures renewable diesel in Illinois for use by a rail carrier. Provides that the amount of the credit is $1 per gallon of renewable diesel that is manufactured by the taxpayer in the State for use by a rail carrier. Effective immediately."
    }
  ],
