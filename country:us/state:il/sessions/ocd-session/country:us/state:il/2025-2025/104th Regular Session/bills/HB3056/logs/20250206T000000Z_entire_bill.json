  "identifier": "HB3056",
  "title": "DOG IMPORT-AGE LIMIT",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Animal Welfare Act. Provides that a person licensed under the Act may not import any dog into the State if the licensee knows or has reason to know that the dog is under the age of 8 weeks old. Provides that a person licensed under the Act must inquire about the age of a dog before importing it into the State."
    }
  ],
