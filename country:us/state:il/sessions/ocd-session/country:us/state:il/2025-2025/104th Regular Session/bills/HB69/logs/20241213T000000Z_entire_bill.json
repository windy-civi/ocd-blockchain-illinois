{
  "legislative_session": "104th",
  "identifier": "HB69",
  "title": "PROP TX-SENIOR FREEZE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Property Tax Code. Provides that, for taxable years 2025 and thereafter, the maximum income limitation for the Low-Income Senior Citizens Assessment Freeze Homestead Exemption is $75,000 for all qualified property. Effective immediately."
    }
  ],
