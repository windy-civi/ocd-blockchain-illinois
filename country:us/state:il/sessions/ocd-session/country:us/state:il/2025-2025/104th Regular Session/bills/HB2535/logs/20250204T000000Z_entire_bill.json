  "title": "UTILITY FIRE PROTECTION FEE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Public Utilities Act. Provides that a fire protection charge by a public utility shall be no greater than the average cost spent by the public utility on the maintenance, connection, or use of any fire protection services over the past 10 years."
    }
  ],
