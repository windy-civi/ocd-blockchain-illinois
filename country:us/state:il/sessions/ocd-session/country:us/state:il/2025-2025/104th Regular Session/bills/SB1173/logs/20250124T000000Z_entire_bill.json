{
  "legislative_session": "104th",
  "identifier": "SB1173",
  "title": "ID CARD-HOMELESS AFFIRMATION",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Identification Card Act. Removes a provision that requires the application of a homeless individual for an Illinois Identification Card to be accompanied by an affirmation by a qualified person, on a form provided by the Secretary of State, that the applicant is currently homeless."
    }
  ],
