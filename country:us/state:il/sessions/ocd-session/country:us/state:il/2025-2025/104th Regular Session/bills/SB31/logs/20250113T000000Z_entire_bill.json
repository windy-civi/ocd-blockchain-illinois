{
  "legislative_session": "104th",
  "identifier": "SB31",
  "title": "COURT REVIEW-PERMANENCY HEARNG",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Juvenile Court Act of 1987. Makes technical changes in a provision that states if the minor is in the custody of the Illinois Department of Children and Family Services, pursuant to an order entered under the Delinquent Minors Article of the Act, the court shall conduct permanency hearings as set out in the court review provisions of the Abused, Neglected, or Dependent Minors Article of the Act."
    }
  ],
