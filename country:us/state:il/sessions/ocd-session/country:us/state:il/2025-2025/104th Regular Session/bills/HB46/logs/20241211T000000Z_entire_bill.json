  "title": "DAY CARE LICENSE APPLICATIONS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Child Care Act of 1969. Adds a provision that requires the Department of Early Childhood to make appropriate accommodations when an applicant for a license to operate a day care center, day care home, or group day care home requests that the application be offered in a language other than English."
    }
  ],
