{
  "legislative_session": "104th",
  "identifier": "HR170",
  "title": "EQUAL PAY DAY",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Declares March 25, 2025 as Equal Pay Day in the State of Illinois. Encourages the citizens of this State to learn about the pay gap that women experience in society and to learn about these various dates that bring attention to the pay gap that specific groups experience."
    }
  ],
