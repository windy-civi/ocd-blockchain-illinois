{
  "legislative_session": "104th",
  "identifier": "HB2400",
  "title": "CRIM CD-SEXUAL ASSAULT",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Criminal Code of 2012. Provides that the sentence for aggravated criminal sexual assault, which does not otherwise provide for an enhanced penalty, is a Class X felony for which 5 years shall be added to the term of imprisonment imposed by the court. Provides that the sentence for predatory criminal sexual assault of a child, which does not otherwise provide for an enhanced penalty, is a Class X felony with a minimum term of imprisonment of 11 (rather than 6) years."
    }
  ],
