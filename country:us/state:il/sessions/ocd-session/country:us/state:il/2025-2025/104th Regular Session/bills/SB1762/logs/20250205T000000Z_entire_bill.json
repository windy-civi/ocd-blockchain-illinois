{
  "legislative_session": "104th",
  "identifier": "SB1762",
  "title": "$DCEO-LOCAL GOV GRANTS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Appropriates $10,000,000 from the General Revenue Fund to the Department of Commerce and Economic Opportunity for the purpose of awarding grants to units of local government for the demolition or repair of commercial buildings that are detrimental to public health or safety as a result of one or more of the following: dilapidation; obsolescence; deterioration; or the failure of the building to meet minimum building code standards. Effective immediately."
    }
  ],
