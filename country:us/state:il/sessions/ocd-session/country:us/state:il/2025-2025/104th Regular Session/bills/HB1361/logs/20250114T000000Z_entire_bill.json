  "identifier": "HB1361",
  "title": "INC TX-SMALL BUSINESS CREDIT",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Income Tax Act. Creates a credit for certain small businesses in an amount equal to the lesser of (i) 10% of the property taxes paid by the qualified small business during the taxable year for eligible real property or (ii) $1,500. Effective immediately."
    }
  ],
