{
  "legislative_session": "104th",
  "identifier": "HB32",
  "title": "TOWNSHIP SUPERVISOR VACANCY",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Township Code. Provides that, in the event of a vacancy in the office of township supervisor, the township trustee who has the longest term of continuous service as a township trustee shall be ex officio supervisor of general assistance in the township and shall administer the general assistance program in the township as provided in Articles VI, XI, and XII of the Illinois Public Aid Code during the pendency of the vacancy in the office of township supervisor."
    }
  ],
