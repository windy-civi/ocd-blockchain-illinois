{
  "legislative_session": "104th",
  "identifier": "SB1941",
  "title": "VEH CD-FLASHING LIGHTS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Allows a unit of local government that has ownership or control over an intersection that is adjacent to property that is used for an elementary or secondary school to install at the ends of the pedestrian crosswalks for that intersection rapid flashing beacons that alert motor vehicle drivers to the presence of pedestrians in the crosswalk when the pedestrians who are using the crosswalk activate the beacons."
    }
  ],
