  "identifier": "SB230",
  "title": "INS-NONPARTICIPATING PROVIDERS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Insurance Code. In a provision concerning services provided by nonparticipating providers, provides that \"health care facility\" in the context of non-emergency services, includes a facility or office in which a patient receives reproductive health care, as defined in the Reproductive Health Act."
    }
  ],
