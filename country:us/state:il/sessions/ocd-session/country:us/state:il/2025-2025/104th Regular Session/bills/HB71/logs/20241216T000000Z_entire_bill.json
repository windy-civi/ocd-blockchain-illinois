{
  "legislative_session": "104th",
  "identifier": "HB71",
  "title": "AGG DOMESTIC BATTERY-PENALTY",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Criminal Code of 2012. Provides that the penalty for aggravated domestic battery is a Class X felony for which the person shall be sentenced to a mandatory term of imprisonment of not less than 6 years and not more than 30 years when the person, in committing a domestic battery, strangles another individual."
    }
  ],
