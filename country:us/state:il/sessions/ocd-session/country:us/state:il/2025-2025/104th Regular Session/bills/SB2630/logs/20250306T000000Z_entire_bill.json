  "identifier": "SB2630",
  "title": "$DNR-DUCKS UNLIMITED",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Appropriates $2,000,000 from the General Revenue Fund to the Department of Natural Resources for grants to Ducks Unlimited, Inc. to fund wetland protection, enhancement, and restoration projects in the State of Illinois, to fund education and outreach for media, volunteers, members, and the general public regarding waterfowl and wetlands conservation in the State of Illinois, and to cover reasonable cost for Ducks Unlimited plate advertising and administration of the wetland conservation projects and education program."
    }
  ],
