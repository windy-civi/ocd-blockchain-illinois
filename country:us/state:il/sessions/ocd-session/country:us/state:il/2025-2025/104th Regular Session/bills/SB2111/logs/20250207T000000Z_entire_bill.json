{
  "legislative_session": "104th",
  "identifier": "SB2111",
  "title": "VEH CD-BICYCLES-EXEMPTIONS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Provides that a person operating a bicycle on the roadways of this State shall not be prohibited from side-by-side riding, riding contraflow on one-way streets, and rolling through stop signs at clear intersections."
    }
  ],
