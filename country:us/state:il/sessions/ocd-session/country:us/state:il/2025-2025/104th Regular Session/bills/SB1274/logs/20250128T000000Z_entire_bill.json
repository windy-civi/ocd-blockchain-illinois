{
  "legislative_session": "104th",
  "identifier": "SB1274",
  "title": "VOUCHER-TAXI & CAR-SHARE",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Sexual Assault Survivors Emergency Treatment Act. Provides that a sexual assault services voucher may be used to seek payment for, among other things, taxi services and car-sharing programs. Defines \"car-sharing program\"."
    }
  ],
