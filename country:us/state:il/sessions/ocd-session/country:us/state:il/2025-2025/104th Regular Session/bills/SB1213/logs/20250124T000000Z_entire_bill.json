{
  "legislative_session": "104th",
  "identifier": "SB1213",
  "title": "CONCEAL CARRY-PUBLIC TRANSPORT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Firearm Concealed Carry Act. Eliminates a provision that a licensee under the Act shall not knowingly carry a firearm on any bus, train, or form of transportation paid for in whole or in part with public funds, or in any building, real property, or parking area under the control of a public transportation facility paid for in whole or in part with public funds. Effective immediately."
    }
  ],
