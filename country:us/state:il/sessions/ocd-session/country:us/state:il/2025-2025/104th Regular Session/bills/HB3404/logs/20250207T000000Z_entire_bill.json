  "identifier": "HB3404",
  "title": "LIGHTED STOP SIGN INSTALL",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Provides that the Department of Transportation shall install a lighted stop sign at any intersection where 2 stop sign violations that resulted in either a fatality or an infliction of great bodily harm have occurred within a 3-month period."
    }
  ],
