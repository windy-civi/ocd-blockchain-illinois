  "identifier": "HJR12",
  "title": "ED HEALTH INSURANCE TASK FORCE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Creates the Educator Health Insurance Task Force to study the feasibility of creating regional health insurance purchasing pools of school districts accessible to all public educators in school districts that have a population of less than 500,000."
    }
  ],
