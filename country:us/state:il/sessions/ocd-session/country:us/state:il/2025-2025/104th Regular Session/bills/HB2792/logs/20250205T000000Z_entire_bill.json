  "title": "FOIA-USER VERIFICATION",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Freedom of Information Act. Provides that, if a public body uses an electronic system for the submission of requests under the Act, then it shall employ a CAPTCHA test or other similar measures to verify that those electronically submitted requests are being made by a human."
    }
  ],
