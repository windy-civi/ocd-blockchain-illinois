{
  "legislative_session": "104th",
  "identifier": "SB2469",
  "title": "OPIOIDS-VETERINARIAN PRESCRIPT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Controlled Substances Act. Provides that, notwithstanding any other provision of law, a prescriber shall not be required to issue prescriptions for a substance classified in Schedule II, III, IV, or V electronically if the prescription is issued by a licensed veterinarian within 7 (rather than 2) years after November 17, 2023 (the effective date of Public Act 103-563)."
    }
  ],
