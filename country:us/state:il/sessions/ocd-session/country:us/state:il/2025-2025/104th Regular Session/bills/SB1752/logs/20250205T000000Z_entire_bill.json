{
  "legislative_session": "104th",
  "identifier": "SB1752",
  "title": "PEN CD-CHI MUNI-RECORDS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Chicago Municipal Article of the Illinois Pension Code. Provides that the board of the fund has the power to have any records kept by the board photographed, microfilmed, or digitally or electronically reproduced in accordance with the Local Records Act. Provides that the photographs, microfilm, and digital and electronic reproductions shall be deemed original records and documents for all purposes, including introduction in evidence before all courts and administrative agencies. Effective immediately."
    }
  ],
