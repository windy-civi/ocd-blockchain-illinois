  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Income Tax Act. Creates an income tax credit for contributions to an ABLE account. Provides that the credit shall be in an amount equal to 25% of that matching contribution, but not to exceed $500 per contributing employee per taxable year. Effective immediately."
    }
  ],
