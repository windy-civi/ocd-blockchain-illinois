{
  "legislative_session": "104th",
  "identifier": "HJR40",
  "title": "COST OF LIVING TASK FORCE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Establishes the Saving Working Families and Easing the Cost of Living Task Force to assess the feasibility of implementing policies focused on reducing the cost of living and enhancing the economic well-being of middle-class and modest-income families."
    }
  ],
