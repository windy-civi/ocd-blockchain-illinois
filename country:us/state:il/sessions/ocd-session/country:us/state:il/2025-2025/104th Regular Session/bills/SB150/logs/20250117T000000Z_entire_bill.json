  "title": "ELEC CD-AI ADVERT DISCLOSURE",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Election Code. Provides that, if a person, committee, or other entity creates, originally publishes, or originally distributes a qualified political advertisement, the qualified political advertisement shall include, in a clear and conspicuous manner, a statement that the qualified political advertisement was generated in whole or substantially by artificial intelligence that satisfies specified requirements. Provides for civil penalties and exceptions to the provision."
    }
  ],
