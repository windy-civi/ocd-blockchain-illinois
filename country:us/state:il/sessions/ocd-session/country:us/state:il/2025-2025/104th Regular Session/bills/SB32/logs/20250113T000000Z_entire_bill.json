{
  "legislative_session": "104th",
  "identifier": "SB32",
  "title": "SNAP BENEFITS-VETERANS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Administration Article of the Illinois Public Aid Code. Provides that subject to federal approval if required, a household that includes one or more veterans or members of the armed forces shall be considered categorically eligible for Supplemental Nutrition Assistance Program benefits if the gross income of such a household is at or below 200% of the nonfarm income poverty guidelines."
    }
  ],
