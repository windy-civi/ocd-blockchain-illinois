{
  "legislative_session": "104th",
  "identifier": "HB1075",
  "title": "BUDGET IMPLEMENTATION ACT",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the State Commemorative Dates Act. Provides that, in accordance with the official day recognized by the government of the Republic of India, the 15th day of the Hindu lunisolar month of Kartika is designated as Diwali Day, to be observed throughout the State by the many Illinoisans who celebrate Diwali, also known as the Festival of Lights, as a day of thanksgiving and reflection."
    }
  ],
