{
  "legislative_session": "104th",
  "identifier": "SB1837",
  "title": "TIMBER BUYERS-HARVEST FEES",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Timber Buyers Licensing Act. Removes language that requires a person buying timber from a timber grower to deduct from the payment to the timber grower an amount which equals 4% of the purchase price or 4% of the minimum fair market value and forward such amount to the Department of Natural Resources. Removes language that requires a timber grower who utilizes timber produced on land the timber grower owns or operates for sawing into lumber, processing, or resale to pay to the Department an amount equal to 4% of the minimum fair market value of the timber utilized during a period. Makes conforming changes."
    }
  ],
