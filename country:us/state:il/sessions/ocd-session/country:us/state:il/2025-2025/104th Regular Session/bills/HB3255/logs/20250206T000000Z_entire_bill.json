{
  "legislative_session": "104th",
  "identifier": "HB3255",
  "title": "PUBLIC SAFETY BENEFITS-SPOUSES",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Public Safety Employee Benefits Act. Provides that, if the injured employee subsequently dies, the employer shall continue to pay the entire health insurance premium for the surviving spouse (rather than the surviving spouse until remarried) and for the dependent children under specified conditions."
    }
  ],
