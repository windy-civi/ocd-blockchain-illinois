  "title": "FEE WAIVER-FORMER INCARCERATED",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Civil Administrative Code of Illinois. Provides that all State agencies shall waive any fees assessed to a formerly incarcerated person for services provided by that State agency for a period of one year after the release of that person from a facility operated by the Department of Corrections, the Department of Juvenile Justice, or the Federal Bureau of Prisons."
    }
  ],
