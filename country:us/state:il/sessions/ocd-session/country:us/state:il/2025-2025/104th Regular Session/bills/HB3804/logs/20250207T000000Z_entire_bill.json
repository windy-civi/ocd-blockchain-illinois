  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Insurance Code. Requires insurance policies to be written in language easily readable and understandable by a person of average intelligence and education. Provides the factors the Director of Insurance shall consider in making the determination that the policy is easily readable and understandable by a person of average intelligence and education."
    }
  ],
