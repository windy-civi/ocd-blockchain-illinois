  "identifier": "SB1345",
  "title": "HIGHER ED-IL VETERAN GRANTS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Higher Education Student Assistance Act. Provides that a qualified applicant for the Illinois Veteran grant program includes a person who, among other requirements, served less than one year of federal active duty and received an uncharacterized discharge as a result of a service-connected disability."
    }
  ],
