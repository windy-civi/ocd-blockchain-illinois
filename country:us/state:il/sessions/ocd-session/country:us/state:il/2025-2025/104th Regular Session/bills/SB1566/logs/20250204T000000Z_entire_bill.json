  "title": "PART-TIME POLICE TRAINING",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Intergovernmental Law Enforcement Officer's In-Service Training Act. Provides that, to be eligible to receive State funds, a Mobile Team In-Service Training Unit must provide a part-time police training course at least every 12 months in a manner approved by the Illinois Law Enforcement Training Standards Board."
    }
  ],
