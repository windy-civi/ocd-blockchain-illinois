{
  "legislative_session": "104th",
  "identifier": "SB1909",
  "title": "QUICK-TAKE-KANE COUNTY",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Eminent Domain Act. Provides that quick-take proceedings may be used for a period of one year after the effective date of the amendatory Act by the Kane County Division of Transportation for the purpose of intersection realignment and separation improvement.. Effective immediately."
    }
  ],
