{
  "legislative_session": "104th",
  "identifier": "HB3710",
  "title": "CRIME VICTIMS RIGHTS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Rights of Crime Victims and Witnesses Act. Provides that crime victims have the right to be treated with fairness and respect during the investigatory process, including the right to be free from deception and free from the knowing communication of false facts about evidence during interviews and conversations with law enforcement."
    }
  ],
