{
  "legislative_session": "104th",
  "identifier": "SB256",
  "title": "WILDLIFE CD-KILLING CONTESTS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Wildlife Code. Provides that a person shall not organize, sponsor, promote, conduct, or participate in any contest, organized competition, tournament, or derby that has as its objective the taking of a fur-bearing mammal for cash, prizes, or other inducement. Provides an exception for field trials, certain wildlife damage control activities, and certain lawful hunting or trapping activities. Establishes penalties for violations."
    }
  ],
