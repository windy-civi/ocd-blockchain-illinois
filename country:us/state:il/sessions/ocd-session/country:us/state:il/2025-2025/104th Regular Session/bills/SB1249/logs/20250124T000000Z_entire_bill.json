{
  "legislative_session": "104th",
  "identifier": "SB1249",
  "title": "VEH CD-EMERGENCY VEHICLES",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Provides that, notwithstanding any other provision of the Code or any rule adopted under the Code, a nonresident who is employed as a public safety worker may operate in this State an authorized emergency vehicle if the nonresident possesses the license class required by the nonresident's state of residence to operate similar equipment and the nonresident has completed the Fire Service Vehicle Operator program of the Illinois State Fire Marshal."
    }
  ],
