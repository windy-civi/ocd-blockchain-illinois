{
  "legislative_session": "104th",
  "identifier": "SB1859",
  "title": "CLIMATE DISPLACEMENT ACT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Creates the Climate Displacement Task Force Act for the purpose of making recommendations regarding climate displacement within the State. Contains provisions concerning the appointment of members to the Task Force and the duties of the Task Force. Requires the Task Force to submit to the General Assembly an initial report delineating the Task Force's findings, conclusions, and recommendations by no later than June 30, 2026."
    }
  ],
