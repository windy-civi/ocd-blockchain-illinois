{
  "legislative_session": "104th",
  "identifier": "SB1256",
  "title": "VEH CD-DISABILITY PLATE/DECAL",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Provides that if a permanently disabled applicant is issued registration plates or a parking decal or device under the Code, the applicant shall submit proof of the applicant's permanent disability once every 5 years. Defines \"proof of the applicant's permanent disability\"."
    }
  ],
