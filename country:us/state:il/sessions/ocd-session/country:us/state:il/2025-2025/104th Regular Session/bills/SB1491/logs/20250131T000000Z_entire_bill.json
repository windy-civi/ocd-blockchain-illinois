{
  "legislative_session": "104th",
  "identifier": "SB1491",
  "title": "INTERVENTION TEAM-THERAPY DOG",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Police Training Act. Requires the Illinois Law Enforcement Training Standards Board to develop a course and certification program for certified therapy dog teams consisting of officers employing the use of therapy dogs in relation to crisis and emergency response."
    }
  ],
