  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Appropriates $1,000,000,000 from the Build Illinois Bond Fund to the Department of Natural Resources for construction and maintenance of State owned, leased, and managed recreation sites and $500,000,000 from the Build Illinois Bond Fund to the Department of Commerce and Economic Opportunity for grants for capital improvements to convention centers. Effective July 1, 2025."
    }
  ],
