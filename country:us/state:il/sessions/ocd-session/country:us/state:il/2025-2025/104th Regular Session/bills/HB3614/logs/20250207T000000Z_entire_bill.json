{
  "legislative_session": "104th",
  "identifier": "HB3614",
  "title": "EPA-CARBON CAPTURE-AQUIFER",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Environmental Protection Act. Provides that no person shall conduct a carbon sequestration activity within a sequestration facility that overlies, underlies, or passes through a sole-source aquifer. Defines \"sole-source aquifer\". Effective immediately."
    }
  ],
