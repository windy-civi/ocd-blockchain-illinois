  "identifier": "HB3558",
  "title": "PROCUREMENT-HIGHER EDUCATION",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Procurement Code. Provides that the Code does not apply to the following procurements made by or on behalf of public institutions of higher education: (1) non-construction procurements under $250,000; (2) construction procurements under $500,000; (3) procurements and purchases made under the Illinois Public Higher Education Cooperative; and (4) all post-award procurement activities and documentation."
    }
  ],
