  "identifier": "HB74",
  "title": "NO PROBATION FOR SEX OFFENDERS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Unified Code of Corrections. Provides that a period of probation, a term of periodic imprisonment, or conditional discharge shall not be imposed for a felony offense that requires registration under the Sex Offender Registration Act."
    }
