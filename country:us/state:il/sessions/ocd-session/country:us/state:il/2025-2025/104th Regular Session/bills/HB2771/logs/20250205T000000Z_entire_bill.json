{
  "legislative_session": "104th",
  "identifier": "HB2771",
  "title": "DPH-CERTIFICATE FEES",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Food, Drug and Cosmetic Act. Provides that the Department of Public Health is authorized, upon request, to issue certificates of free sale, health certificates, or an equivalent, to Illinois food, dairy, drug, cosmetic, or medical device manufacturers, processors, packers, or warehousers for a fee of $65 (rather than $10)."
    }
  ],
