{
  "legislative_session": "104th",
  "identifier": "SB2149",
  "title": "SCH CD-TUITION FOR ORPHANS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the School Code. Provides that the depreciation allowance included under the amount of the tuition for a child from a home for orphans, dependent, abandoned, or maladjusted children shall exclude depreciation of assets paid from federal or donated funds."
    }
  ],
