  "title": "FIREARM OWNERS ID-18 YEARS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Firearm Owners Identification Card Act. Lowers the age at which a person may apply for a Firearm Owner's Identification Card without parental or legal guardian consent from 21 years of age to 18 years of age. Makes corresponding changes."
    }
  ],
