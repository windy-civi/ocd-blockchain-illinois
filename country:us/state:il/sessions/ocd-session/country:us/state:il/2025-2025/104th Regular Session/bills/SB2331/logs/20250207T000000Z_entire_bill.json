  "legislative_session": "104th",
  "identifier": "SB2331",
  "title": "LIVESTOCK FACILITY SPACING",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Livestock Management Facilities Act. Provides that a new facility created or expanded after the effective date of the amendatory Act that serves 50 or greater animal units shall have a minimum setback distance of 2 miles from the nearest occupied residence and 2 miles from the nearest populated area."
    }
  ],
