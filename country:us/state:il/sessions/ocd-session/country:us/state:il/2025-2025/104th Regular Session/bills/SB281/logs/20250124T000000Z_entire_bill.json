{
  "legislative_session": "104th",
  "identifier": "SB281",
  "title": "FOID-MILITARY OUT OF STATE",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Firearm Owners Identification Card Act. Provides that the authority of the Illinois State Police to deny an application for or to revoke and seize a Firearm Owner's Identification Card previously issued under the Act to a person who is not a resident of the State of Illinois does not apply to an applicant who is an Illinois resident in the United States Military permanently assigned out of state. Effective immediately."
    }
  ],
