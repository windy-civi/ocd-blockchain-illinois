{
  "legislative_session": "104th",
  "identifier": "SJR31",
  "title": "MANUFACTURING INDUSTRY-SUPPORT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Reaffirms a dedication to supporting the efforts of small and midsize manufacturers across Illinois. Urges the small and the midsize manufacturing industries and other relevant stakeholders to form a task force to develop meaningful recommendations for addressing the needs of small and midsize manufacturers in Illinois. Reaffirms a commitment to championing initiatives that ensure the success of small and midsize manufacturers in Illinois."
    }
  ],
