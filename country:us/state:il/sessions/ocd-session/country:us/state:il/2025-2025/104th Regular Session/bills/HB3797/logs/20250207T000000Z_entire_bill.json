  "title": "SCH CD-SUSPEND TEACHER LICENSE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Educator Licensure Article of the School Code. Requires the State Board of Education to create a system for the automatic suspension of a license if the holder of that license has been convicted of a crime under the Sex Offenses Article of the Criminal Code of 2012 or a crime that requires registration under the Sex Offender Registration Act."
    }
  ],
