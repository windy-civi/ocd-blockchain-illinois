  "identifier": "SB1466",
  "title": "CORPORATION-INCORPORATION DATE",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Business Corporation Act of 1983. Provides that the articles of incorporation of any corporation (currently, any corporation incorporated after December 31, 1981) may limit or eliminate cumulative voting rights in all or specified circumstances, or may limit or deny voting rights or may provide special voting rights as to any class or classes or series of shares of such corporation."
    }
  ],
