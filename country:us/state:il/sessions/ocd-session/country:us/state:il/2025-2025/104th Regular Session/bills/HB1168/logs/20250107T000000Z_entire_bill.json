{
  "legislative_session": "104th",
  "identifier": "HB1168",
  "title": "DAY CARE CENTER-CIVIC ORG",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Child Care Act of 1969. Provides that the definition of \"day care center\" does not include special activities programs that are conducted by civic, charitable, and governmental organizations on an organized basis (instead of special activities programs that are conducted on an organized and periodic basis). Adds programs offered by arboretums, nature centers, and botanic gardens to the special activities programs that are excluded from the definition of \"day care center\"."
    }
  ],
