  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Counties Code. Provides that county recorders and registrars of title are required to accept payment of real estate transfer taxes by credit card. Provides that all service charges or fees associated with the processing or accepting of such a credit card payment shall be paid by the taxpayer."
    }
  ],
