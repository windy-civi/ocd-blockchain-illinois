{
  "legislative_session": "104th",
  "identifier": "HR404",
  "title": "ENCOURAGES-LIGHT REGULATION",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Encourages State agencies to regulate the use of electrical lights in State buildings and facilities to better conserve energy and to minimize light pollution. Urges electrical lights in State buildings and facilities to be turned off unless the building or facility or that part of it is occupied, unless the building or facility is open 24-hours a day, limited to the areas and floors that are occupied, or unless the lighting is required for security or safety purposes."
    }
  ],
