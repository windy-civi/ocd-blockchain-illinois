{
  "legislative_session": "104th",
  "identifier": "SB1196",
  "title": "LOBBYISTS-FEE WAIVER",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Lobbyist Registration Act. Directs the Secretary of State to grant a waiver of the lobbyist registration fee for any not-for-profit entity with total expenses of less than $1,000,000 in the most recent fiscal year that is classified as tax-exempt under Section 501(c)(3) of the Internal Revenue Code, including a waiver for any lobbyist that exclusively lobbies on behalf of such an entity."
    }
  ],
