  "identifier": "HB3094",
  "title": "TRANSPORTATION BENEFIT PROGRAM",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Transportation Benefits Program Act. Provides that the Act does not apply to a covered employer in the construction industry with respect to employees with whom the covered employer has entered into a bona fide collective bargaining agreement. Makes changes to definitions."
    }
  ],
