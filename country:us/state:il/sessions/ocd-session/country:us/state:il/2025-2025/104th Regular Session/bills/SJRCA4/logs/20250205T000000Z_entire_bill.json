{
  "legislative_session": "104th",
  "identifier": "SJRCA4",
  "title": "CONAMEND-INCOME TAX RATES",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "constitutional amendment"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Proposes to amend the Revenue Article of the Illinois Constitution. Removes a provision that provides that a tax on income shall be measured at a non-graduated rate. Provides that the General Assembly shall provide by law for the rate or rates of any tax on or measured by income imposed by the State (currently, there may be no more than one such tax imposed by the State on individuals and one such tax so imposed on corporations). Provides that the highest rate of tax imposed on corporations may not exceed the highest rate imposed on individuals by more than a ratio of 8 to 5. Effective upon being declared adopted."
    }
  ],
