{
  "legislative_session": "104th",
  "identifier": "SB69",
  "title": "INS CODE-RIDING THERAPY",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Insurance Code. Provides that a group or individual policy of accident and health insurance or managed care plan that is amended, delivered, issued, or renewed after the effective date of the amendatory Act shall provide coverage for hippotherapy and other forms of therapeutic riding. Makes conforming changes in the State Employees Group Insurance Act of 1971, the Counties Code, the Illinois Municipal Code, the School Code, and the Health Maintenance Organization Act."
    }
  ],
