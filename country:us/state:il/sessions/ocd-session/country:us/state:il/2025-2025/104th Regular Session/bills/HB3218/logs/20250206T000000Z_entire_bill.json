{
  "legislative_session": "104th",
  "identifier": "HB3218",
  "title": "SALE OF E-15 GASOLINE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Motor Fuel Sales Act. Authorizes motor fuel blends containing 10.5% to 15% ethanol, by volume, to be sold in the State at any time during the year if permitted under federal law. Provides that, if a federal authorization must be obtained to provide for the year-round sale of those motor fuel blends, then the Illinois Environmental Protection Agency shall take all actions necessary to obtain the federal authorization on behalf of the State."
    }
  ],
