{
  "legislative_session": "104th",
  "identifier": "HJR25",
  "title": "JIM EDGAR READING ROOM",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Designates Rooms 105 and 109 of the Illinois State Library at 300 South Second Street in Springfield as the \"Jim Edgar Reading Room\" that houses the Illinois Authors' Collection in honor of the lasting legacy and impact of former Illinois Governor and Illinois Secretary of State Jim Edgar."
    }
  ],
