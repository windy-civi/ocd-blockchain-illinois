{
  "legislative_session": "104th",
  "identifier": "SB1420",
  "title": "LOCAL HEALTH DEPT-RMSF",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Department of Public Health Powers and Duties Law of the Civil Administrative Code of Illinois. Provides that the Department of Public Health shall adopt rules requiring a local health department to make public announcements for the purpose of informing the general public when a positive case of Rocky Mountain Spotted Fever is detected within a county or area under the local health department's jurisdiction."
    }
  ],
