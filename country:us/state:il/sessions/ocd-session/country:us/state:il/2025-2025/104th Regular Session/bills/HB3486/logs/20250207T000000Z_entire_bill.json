{
  "legislative_session": "104th",
  "identifier": "HB3486",
  "title": "MOORISH-AMERICAN MONTH",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the State Commemorative Dates Act. Designates the month of August of each year as Moorish American Awareness Month to be observed throughout the State as a month to recognize the valuable contributions of Moorish Americans to this State and to the various aspects of American society. Effective immediately."
    }
  ],
