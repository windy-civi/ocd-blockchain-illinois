  "identifier": "SB1842",
  "title": "MUNI-PART-TIME POLICE OFFICERS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Municipal Code. Provides that part-time police officers shall not be assigned to supervise or direct full-time police officers of a police department, except in municipalities having a population of less than 3,000 (rather than part-time police officers shall not be assigned under any circumstances to supervise or direct full-time police officers of a police department). Effective immediately."
    }
  ],
