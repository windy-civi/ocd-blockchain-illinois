{
  "legislative_session": "104th",
  "identifier": "HB1166",
  "title": "UTILITIES-JOB TRAINING CENTERS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Public Utilities Act. Provides that an electric utility that serves more than 3,000,000 customers in the State shall fund the construction of 5 employment training centers at a cost to be determined by the utility. Provides that the employment training centers shall be located in: the west side of Chicago; Ford Heights; Waukegan; Bloomington; and Peoria."
    }
  ],
