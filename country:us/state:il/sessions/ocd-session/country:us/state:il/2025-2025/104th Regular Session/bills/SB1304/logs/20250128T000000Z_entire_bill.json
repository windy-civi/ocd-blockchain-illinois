  "identifier": "SB1304",
  "title": "REDUCING BARRIERS TO START ACT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Creates the Reducing Barriers to Start Act. Beginning January 1, 2026, this State shall encourage the elimination of all first-year business fees relating to any license or registration for any new business or person establishing a new business, including home-based businesses, whose principal place of business is in this State."
    }
  ],
