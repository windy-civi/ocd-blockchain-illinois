{
  "legislative_session": "104th",
  "identifier": "HB1169",
  "title": "501(C)(3) LOBBYING FEE EXEMPT",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Lobbyist Registration Act. Directs the Secretary of State to grant a waiver of the lobbyist registration fee for any not-for-profit entity with an annual budget of less than $5,000,000 that is classified as tax-exempt under Section 501(c)(3) of the Internal Revenue Code, including a waiver for any lobbyist that exclusively lobbies on behalf of such an entity."
    }
  ],
