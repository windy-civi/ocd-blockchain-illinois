  "identifier": "HB1321",
  "title": "PROP TX-TAX DISTRICT SURPLUS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Property Tax Code. Provides that no taxing district may hold more than 150% of the previous levy year's property tax collections in cash or cash-equivalent assets. Provides that excess amounts shall be refunded to taxpayers. Effective immediately."
    }
  ],
