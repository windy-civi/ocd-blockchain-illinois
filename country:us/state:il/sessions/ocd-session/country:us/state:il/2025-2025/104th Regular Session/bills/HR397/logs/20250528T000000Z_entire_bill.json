  "title": "PUERTO RICO-FISCAL AUTONOMY",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Urges the United States Congress to take immediate action to establish a definitive end date for the operation of the Puerto Rico Financial Oversight and Management Board, ensure a transition toward full fiscal autonomy and local democratic governance in Puerto Rico, and promote economic self actualization and just recovery efforts that are community-driven and accountable to Puerto Ricans on the island."
    }
  ],
