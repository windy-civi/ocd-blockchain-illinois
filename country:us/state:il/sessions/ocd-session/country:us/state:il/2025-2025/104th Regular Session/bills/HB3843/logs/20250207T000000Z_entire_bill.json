  "identifier": "HB3843",
  "title": "ARTERIAL STREET SETBACK",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Counties Code. Provides that, in counties with a population of over 3,000,000, the minimum setback distance for new developments is 150 feet away from any arterial street. Defines \"arterial street\" for the purposes of the provisions."
    }
  ],
