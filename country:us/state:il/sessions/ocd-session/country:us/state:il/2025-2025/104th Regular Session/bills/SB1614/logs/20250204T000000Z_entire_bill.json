{
  "legislative_session": "104th",
  "identifier": "SB1614",
  "title": "FINANCE-SOS PETTY CASH",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the State Finance Act. Provides that the Secretary of State may maintain a petty cash fund in excess of $1,000, solely for the purpose of making change, at any Secretary of State Motor Vehicle Facility. Provides that those funds may be retained on the premises of the facility."
    }
  ],
