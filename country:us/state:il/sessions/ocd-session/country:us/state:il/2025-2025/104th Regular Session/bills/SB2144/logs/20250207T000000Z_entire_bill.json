  "identifier": "SB2144",
  "title": "COMPETITIVELY NEUTRAL ENERGY",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Creates the Transparent, Inclusive, and Competitively Neutral Energy Policy Act. Sets forth findings. Provides that the energy procurement process shall be transparent, inclusive, and competitively neutral and shall provide due consideration to the unique advantages of each developer from all segments of the energy sector and recognize that more inclusive procurements result in greater competition among developers and lower costs for consumers."
    }
  ],
