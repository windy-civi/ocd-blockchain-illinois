{
  "legislative_session": "104th",
  "identifier": "HB1799",
  "title": "INC TX-WAGES PAID TO SENIORS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Income Tax Act. Provides that a taxpayer is entitled to an income tax credit for each person who is (i) 62 years of age or older during the taxable year and (ii) employed by the taxpayer at a location in this State for not less than 185 days during the taxable year. Provides that the credit may be carried forward. Provides that the credit is exempt from the Act's automatic sunset provision. Effective immediately."
    }
  ],
