{
  "legislative_session": "104th",
  "identifier": "SB2044",
  "title": "WEB-BASED SIGNATURES ACT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Creates the Web-Based Signatures Act. Provides that a unit of local government may allow a person to sign any document with a web-based signature if the unit of local government uses a secure web-based platform."
    }
  ],
