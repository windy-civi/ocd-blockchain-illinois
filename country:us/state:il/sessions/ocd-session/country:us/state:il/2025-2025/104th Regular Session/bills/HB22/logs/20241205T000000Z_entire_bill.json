{
  "legislative_session": "104th",
  "identifier": "HB22",
  "title": "CROHNS AND COLITIS AWARENESS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the State Commemorative Dates Act. Provides that December 1 through December 7 of each year is designated as Crohn's and Colitis Awareness Week as a week to encourage awareness of Crohn's disease and ulcerative colitis."
    }
  ],
