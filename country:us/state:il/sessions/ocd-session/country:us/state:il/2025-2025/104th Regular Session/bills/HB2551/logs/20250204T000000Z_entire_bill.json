{
  "legislative_session": "104th",
  "identifier": "HB2551",
  "title": "FIRST RESPONDER TASK FORCE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the First Responders Suicide Prevention Act. Reinstates the First Responders Suicide Task Force. Adds to the reconstituted Task Force one member who represents an organization that provides mental health training and support for first responders in State of Illinois, appointed by the President of the Senate. Provides that members of the Task Force shall be appointed within 30 days after the effective date of the amendatory Act. Directs the Task Force to issue a final report to the General Assembly on or December 31, 2026. Provides for the repeal of the Task Force on January 1, 2027."
    }
  ],
