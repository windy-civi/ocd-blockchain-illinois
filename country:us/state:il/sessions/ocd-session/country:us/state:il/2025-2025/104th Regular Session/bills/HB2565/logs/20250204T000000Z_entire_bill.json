{
  "legislative_session": "104th",
  "identifier": "HB2565",
  "title": "UNEMPLOYMENT INS-LABOR DISPUTE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Unemployment Insurance Act. Provides that an individual shall be ineligible for benefits for a period totaling and not to exceed 2 weeks (rather than an individual shall be ineligible for benefits for any week) with respect to which it is found that his total or partial unemployment is due to a stoppage of work which exists because of a labor dispute at the factory, establishment, or other premises at which he is or was last employed. Provides that, after the 2-week period, the individual will be eligible for benefits."
    }
  ],
