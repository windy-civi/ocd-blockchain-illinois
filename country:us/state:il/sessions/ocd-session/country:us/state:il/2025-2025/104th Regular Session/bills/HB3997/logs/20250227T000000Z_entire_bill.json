{
  "legislative_session": "104th",
  "identifier": "HB3997",
  "title": "$DCEO/ICCB-GRANTS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Appropriates $5,000,000 from the General Revenue Fund to the Department of Commerce and Economic Opportunity for grants to specified organizations as deemed appropriate by the Department for purposes of administering preapprenticeship workforce development programs that support education and training programming targeted to individuals under the age of 18 who reside in underserved areas in specified fields. Appropriates $5,000,000 from the General Revenue Fund to the Illinois Community College Board for grants to community college districts and public high schools for the purposes of administering the 21st Century Employment grant program. Effective July 1, 2025."
    }
  ],
