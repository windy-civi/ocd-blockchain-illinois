{
  "legislative_session": "104th",
  "identifier": "SB1230",
  "title": "CAPITAL DEVELOPMENT-REPORT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Capital Development Board Act. Provides that, on or before July 1, 2026, and every year thereafter, the Capital Development Board shall submit a report to the General Assembly and the Governor concerning all upcoming and proposed projects constructed by or under the supervision of the Board."
    }
  ],
