{
  "legislative_session": "104th",
  "identifier": "HB3065",
  "title": "CPS-DISCIPLINE DISCLOSURE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the School Code. Provides that the parent or guardian of a student against whom misconduct was committed by another student shall receive a copy of the disciplinary report from an investigation into the perpetrating student's act or acts of misconduct."
    }
  ],
