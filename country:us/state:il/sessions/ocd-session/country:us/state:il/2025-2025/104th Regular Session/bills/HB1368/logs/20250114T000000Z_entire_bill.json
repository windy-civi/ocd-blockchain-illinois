  "identifier": "HB1368",
  "title": "SCH CD-PROF ED LICENSE RENEW",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Educator Licensure Article of the School Code. Requires an approved provider of professional development activities for the renewal of a Professional Educator License to ensure that professional development related to English language arts is aligned with the comprehensive literacy plan for the State developed by the State Board of Education. Effective immediately."
    }
  ],
