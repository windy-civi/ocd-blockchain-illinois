  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Provides that the Secretary of State may issue an instruction permit to a child who is not less than 15 years of age if the child receives permission from a driver education teacher or administrator from a school offering an approved driver education course (rather than if the child is enrolled in an approved driver education course)."
    }
  ],
