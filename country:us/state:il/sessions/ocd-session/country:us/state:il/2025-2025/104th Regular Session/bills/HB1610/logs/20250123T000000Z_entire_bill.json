  "title": "HOUSING-MULTI-LINGUAL LEASES",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Housing Authorities Act. Requires every housing authority to post on its official website in a downloadable, printable format sample rental or lease agreements that are written in the 3 most common non-English languages spoken in the city, village, incorporated town, or county for which the housing authority was created."
    }
  ],
