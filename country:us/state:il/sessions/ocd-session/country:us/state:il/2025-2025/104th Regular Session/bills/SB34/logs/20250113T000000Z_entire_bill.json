  "title": "ETHICS-EQUITABLE CASEWORK",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the State Officials and Employees Ethics Act. Prohibits a member from refusing casework service to an individual who resides in the member's district based solely upon the individual's political affiliation. Defines \"casework service\". Effective immediately."
    }
  ],
