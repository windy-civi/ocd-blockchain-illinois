{
  "legislative_session": "104th",
  "identifier": "SB2443",
  "title": "DL EXPIRATION-ARMED FORCES",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Provides that the Secretary of State may defer the expiration of the driver's license of a licensee serving in the Armed Forces of the United States outside of the State of Illinois for any length of time that does not exceed 6 months following the discharge of the licensee from the Armed Forces of the United States or within 90 days after reestablishing residence within the State, whichever is sooner (rather than 120 days after the expiration of the driver's license)."
    }
  ],
