  "identifier": "HB3277",
  "title": "EMPLOYEE CREDIT PRIVACY-SSN",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Employee Credit Privacy Act. Provides that an employer shall not order or obtain an applicant's social security number, except for the purpose of conducting a background check of the applicant at the time the background check is completed. Provides that the provision does not prohibit an employer from obtaining an employee's social security number after the employee has been hired."
    }
  ],
