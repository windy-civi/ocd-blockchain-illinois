  "title": "VETS-EMPLOYMENT PREFERENCE",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Veterans Preference in Private Employment Act. Expands the definition of \"veteran\" to include a member of the United States Armed Forces Reserves who has never been deployed but separated under conditions other than dishonorable as noted on the individuals' service (rather than NGB-22) discharge form. Effective immediately."
    }
  ],
