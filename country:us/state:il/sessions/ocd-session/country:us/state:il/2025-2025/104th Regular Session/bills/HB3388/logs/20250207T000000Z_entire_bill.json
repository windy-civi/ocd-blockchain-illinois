{
  "legislative_session": "104th",
  "identifier": "HB3388",
  "title": "VEH CD-FIRE DEPT VEHICLES",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Includes any recreational off-highway vehicle, all-terrain vehicle, watercraft, or aircraft that is designated or authorized by proper local authorities for fire department use in the definition of \"fire department vehicle\". Effective immediately."
    }
  ],
