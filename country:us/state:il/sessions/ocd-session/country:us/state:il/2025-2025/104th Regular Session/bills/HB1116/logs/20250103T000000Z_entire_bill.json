  "identifier": "HB1116",
  "title": "HIGHER ED-STU-ATH ENDORSEMENT",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Student-Athlete Endorsement Rights Act. Provides that if a student-athlete earns more than $250,000 from the use of the name, image, likeness, or voice of the student-athlete, then any academic scholarship granted to the student-athlete shall be reduced by $1 per dollar the student-athlete earns over $250,000."
    }
  ],
