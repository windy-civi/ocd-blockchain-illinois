{
  "legislative_session": "104th",
  "identifier": "SJRCA1",
  "title": "CON AMEND-TERM LIMITS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "constitutional amendment"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Proposes to amend the Legislature and Executive Articles of the Illinois Constitution. Provides that a person may not be elected to the office of State Senator for more than three terms or the office of State Representative for more than five terms, and that service in the General Assembly before the second Wednesday in January of 2025 shall not be considered in the calculation of a person's service. Provides that a person may not be elected to any Executive Branch office, or any combination of Executive Branch offices, for more than 2 terms, and that service in an Executive Branch office before the second Monday in January of 2025 shall not be considered in the calculation of a person's service."
    }
  ],
