  "identifier": "HB3132",
  "title": "NURSE-TUITION REIMBURSEMENT",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Nurse Practice Act. Provides that an individual licensed under this Section may apply for a program that provides a full tuition reimbursement to individuals who: (1) attended and graduated from an education program based in the State; and (2) commit to working in a safety-net hospital in the State for at least 5 years."
    }
  ],
