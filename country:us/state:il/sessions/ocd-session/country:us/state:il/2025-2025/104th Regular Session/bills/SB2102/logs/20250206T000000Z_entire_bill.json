{
  "legislative_session": "104th",
  "identifier": "SB2102",
  "title": "PROP TX-BUDGET FILING",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Property Tax Code. Provides that a taxing district's budget and appropriation ordinance and estimate of revenues may be filed electronically with the county clerk. Provides that, if a taxing district's budget and appropriation ordinance and estimate of revenues are filed electronically with the county clerk, the county clerk shall accept and acknowledge that electronic filing by providing a receipt to the taxing district. Effective immediately."
    }
  ],
