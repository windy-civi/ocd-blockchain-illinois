{
  "legislative_session": "104th",
  "identifier": "SB62",
  "title": "BUILD ILLINOIS HOMES ACT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Creates the Build Illinois Homes Tax Credit Act. Provides that owners of qualified low-income housing developments are eligible for credits against the taxes imposed by the Illinois Income Tax Act or taxes, penalties, fees, charges, and payments imposed by the Illinois Insurance Code. Amends the Illinois Income Tax Act and the Illinois Insurance Code to make conforming changes. Effective immediately."
    }
  ],
