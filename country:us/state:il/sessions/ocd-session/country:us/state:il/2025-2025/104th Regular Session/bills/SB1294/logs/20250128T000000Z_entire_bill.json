  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Procurement Code. Provides that the chief procurement officer may not award a contract to a vendor if more than 10% of the services contracted, based on the cost of services, are to be performed outside of the United States."
    }
  ],
