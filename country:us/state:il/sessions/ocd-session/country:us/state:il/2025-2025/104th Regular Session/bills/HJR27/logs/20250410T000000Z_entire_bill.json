  "title": "GOOD FOOD TASK FORCE",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Creates the Illinois Good Food Purchasing Policy Task Force to study the current procurement of food within the State and explore how Good Food Purchasing can be implemented to maximize the procurement of healthy foods that are sustainably, locally, and equitably sourced."
    }
  ],
