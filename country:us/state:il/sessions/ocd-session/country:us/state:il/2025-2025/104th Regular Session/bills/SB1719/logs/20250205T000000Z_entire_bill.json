{
  "legislative_session": "104th",
  "identifier": "SB1719",
  "title": "LOC GOV-ELECTRONIC RECORDS",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Local Records Act. Provides that a unit of local government required to store public records under the Act may satisfy the requirements of the Act by storing the public records in an electronic form."
    }
  ],
