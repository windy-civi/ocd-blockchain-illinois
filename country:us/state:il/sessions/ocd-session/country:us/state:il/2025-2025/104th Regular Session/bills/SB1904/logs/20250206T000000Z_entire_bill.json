  "title": "ELEC CD-ENFORCEMENT DUTIES",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Election Code. Provides that the State Board of Elections shall exercise the powers to impose campaign disclosure penalties; to hear and adjudicate alleged violations of registration requirements; to revoke or suspend the raffle licenses of political committees that violate the Raffles and Poker Runs Act; and to inform the Attorney General or the State's Attorney of credible alleged criminal violations."
    }
  ],
