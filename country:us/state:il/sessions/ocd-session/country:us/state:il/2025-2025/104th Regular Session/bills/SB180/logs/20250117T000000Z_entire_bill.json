  "legislative_session": "104th",
  "identifier": "SB180",
  "title": "JUDGESHIPS-6TH JUD CIRCUIT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Circuit Courts Act. Provides, in a provision concerning resident judgeships of the 6th judicial circuit, that when a vacancy occurs in an at-large judgeship, the at-large judgeship shall be converted to a resident judgeship for either Champaign County or Macon County, depending on which of those 2 counties the incumbent at-large circuit judge resided in when the incumbent judge was elected. Effective immediately."
    }
  ],
