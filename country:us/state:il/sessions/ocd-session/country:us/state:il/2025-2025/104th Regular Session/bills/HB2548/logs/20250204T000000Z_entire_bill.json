{
  "legislative_session": "104th",
  "identifier": "HB2548",
  "title": "PROHIBIT AT-HOME ASSAULT KITS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Consumer Fraud and Deceptive Business Practices Act. Provides that it is an unlawful practice for any person to sell, market, promote, advertise, or otherwise distribute a self-administered sexual assault evidence collection kit to a consumer in the State. Defines terms."
    }
  ],
