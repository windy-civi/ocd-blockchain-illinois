{
  "legislative_session": "104th",
  "identifier": "HB3671",
  "title": "CRIM CD-VENUE-SEXUAL IMAGES",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Criminal Code of 2012. Provides that a person who commits the offense of non-consensual dissemination of private sexual images may be tried in any one of the following counties in which: (1) the offense occurred; or (2) the victim resides."
    }
  ],
