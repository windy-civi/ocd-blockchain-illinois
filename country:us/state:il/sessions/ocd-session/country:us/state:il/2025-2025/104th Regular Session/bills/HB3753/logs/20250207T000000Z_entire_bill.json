  "legislative_session": "104th",
  "identifier": "HB3753",
  "title": "PREVAILING WAGE TRUCKING",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Prevailing Wage Act. Provides that the transportation of ready-mixed concrete and aggregate materials shall be deemed to be employment upon public works. Includes additional trucking related activities within the scope of the Act. Includes in the calculation of the prevailing wage rate, rental costs for trucks and equipment provided by owner-operators. Defines terms. Effective immediately."
    }
  ],
