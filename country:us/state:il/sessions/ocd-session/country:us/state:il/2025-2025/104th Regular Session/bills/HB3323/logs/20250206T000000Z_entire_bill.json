{
  "legislative_session": "104th",
  "identifier": "HB3323",
  "title": "DEPT OF AG-LOCAL FARM-MOTHER",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Requires the Department of Agriculture to create a grant program to award grants of up to $250,000, subject to appropriation, to cover the costs and labor of any qualified applicant farmer to distribute healthy, free food to expectant mothers in need who reside in the same county or in a municipality within 20 miles of the farmer's farm, with certain requirements. Requires the Department to adopt rules. Defines terms."
    }
  ],
