  "identifier": "SB25",
  "title": "SWIMMING FACILITY COLD SPA",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Swimming Facility Act. Defines \"cold spa\" and provides that it is lawful for a licensee to operate a cold spa in a manner that complies with the provisions of the Act and the rules adopted under the Act."
    }
  ],
