  "identifier": "HB2953",
  "title": "CDB-FIRE SPRINKLERS",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Capital Development Board Act. Provides that each municipality and county shall provide the Capital Development Board a list of all high-rise apartment buildings in their respective jurisdictions that: (1) meet the definition of high-rise apartment building; (2) were not subject to a requirement to include a sprinkler system at the time the building was constructed; and (3) have not been retrofitted with a sprinkler system."
    }
  ],
