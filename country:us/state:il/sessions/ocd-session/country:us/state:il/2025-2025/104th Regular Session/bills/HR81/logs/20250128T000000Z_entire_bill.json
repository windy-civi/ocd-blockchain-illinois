{
  "legislative_session": "104th",
  "identifier": "HR81",
  "title": "WEAR RED DAY",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Declares February 7, 2025 as Wear Red Day in the State of Illinois. Urges all residents of Illinois to raise awareness about cardiovascular disease, the leading cause of death in women, and to show their support for women and the fight against heart disease by wearing the color red to commemorate this day."
    }
  ],
