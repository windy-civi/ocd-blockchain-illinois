  "identifier": "SB1573",
  "title": "ELECTRONIC NOTARY FEE WAIVER",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Notary Public Act. Provides that a fee of $25 for a commission as an electronic notary public shall be waived for a period of 2 years. Provides that a bond of $25,000 for a commission as an electronic notary shall be waived for a period of 2 years, except that a bond of $5,000 shall still be required. Effective immediately."
    }
  ],
