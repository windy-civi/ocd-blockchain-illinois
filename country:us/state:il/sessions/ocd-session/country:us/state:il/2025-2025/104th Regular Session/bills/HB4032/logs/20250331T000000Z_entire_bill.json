{
  "legislative_session": "104th",
  "identifier": "HB4032",
  "title": "NO FUNDS W/OUT REVENUE EST",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Creates the Revenue Estimate Act. Provides that the General Assembly shall not enact any bill to appropriate funds within any fiscal year prior to its adoption of a joint resolution reflecting the estimate of funds available for that fiscal year as required under the Commission on Government Forecasting and Accountability Act. Effective immediately."
    }
  ],
