  "identifier": "SB1647",
  "title": "INC TX-EDUCATION",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Income Tax Act. Creates an income tax credit for taxpayers who are the custodians of one or more children who attend an eligible preschool program in the State during the taxable year. Provides that the amount of the credit shall be 100% of the eligible expenses incurred by the taxpayer during the taxable year in sending the child to the eligible preschool program, but not to exceed $1,500 per child. Effective immediately."
    }
  ],
