{
  "legislative_session": "104th",
  "identifier": "SB1883",
  "title": "VEH CD-OBSTRUCT LICENSE PLATE",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Illinois Vehicle Code. Provides that no person shall affix tape to any registration plate, temporary registration plate, digital registration plate, registration sticker, or other evidence of registration issued by the Secretary of State. Makes a conforming change. Provides that it is unlawful to operate any vehicle that is equipped with a manual, electronic, or mechanical device that hides or obscures a registration plate."
    }
  ],
