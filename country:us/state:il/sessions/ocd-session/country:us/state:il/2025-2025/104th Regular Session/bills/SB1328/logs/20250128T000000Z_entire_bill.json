{
  "legislative_session": "104th",
  "identifier": "SB1328",
  "title": "NON-APPROVED SMOKE DETECTOR",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "",
      "abstract": "Amends the Smoke Detector Act. Bans the selling, offering for sale, or giving as a gift a smoke detector that is not designed to receive primary power from the building wiring or does not meet certain battery requirements set forth in the Act. Specifies that a person who violates this requirement is guilty of a petty offense and is subject to a fine."
    }
  ],
